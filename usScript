-- Scripts:

local function TWBG_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	
	local function getEmotesFolder()
		local folder = player:FindFirstChild("Emotes")
		if folder then return folder end
	
		if player.Character then
			folder = player.Character:FindFirstChild("Emotes")
			if folder then return folder end
		end
	
		player.ChildAdded:Wait()
		return getEmotesFolder()
	end
	
	button.MouseButton1Click:Connect(function()
	
		local emotesFolder = getEmotesFolder()
	
		for _, v in ipairs(emotesFolder:GetChildren()) do
			if v:IsA("BoolValue") then
				v.Value = true
			end
		end
	
	end)
	
end
coroutine.wrap(TWBG_fake_script)()
local function QWQB_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	
	button.MouseButton1Click:Connect(function()
		local ultValue = player:FindFirstChild("Ult")
	
		if ultValue then
			ultValue.Value = 125
		end
	end)
	
end
coroutine.wrap(QWQB_fake_script)()
local function WHWYCQ_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		local remote = game.ReplicatedStorage:WaitForChild("Assets")
			:WaitForChild("EarthsHeroAssets")
			:WaitForChild("Remotes")
			:WaitForChild("Spirit Bomb")
	
		local args = { [1] = "" }
	
		local success, err = pcall(function()
			remote:FireServer(unpack(args))
		end)
	
		if not success then
		end
	end)
	
end
coroutine.wrap(WHWYCQ_fake_script)()
local function EVRPRDP_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	
	button.MouseButton1Click:Connect(function()
		local leaderstats = player:FindFirstChild("leaderstats")
		if not leaderstats then
			return
		end
	
		local totalKills = leaderstats:FindFirstChild("Total Kills")
		if totalKills then
			totalKills.Value = 25000
		end
	end)
	
end
coroutine.wrap(EVRPRDP_fake_script)()
local function MBBS_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(MBBS_fake_script)()

